{
	"Field input": {
		"Shortcut": "Ctrl+M",
		"Rich text shortcut": "Ctrl+Alt+X",
		"Default field state": "rich text",
		"Cycle rich text/Markdown": true,
		"CSS": ".cm-content { font-family: Consolas, monospace; font-size: 16px; }"
	},
	"Dialog input": {
		"Shortcut": "Ctrl+Alt+M",
		"Shortcut accept": "Ctrl+Return",
		"Shortcut reject": "Shift+Escape",
		"Size mode": "parent",
		"Selection only": false,
		"CSS": ".cm-content { font-family: Consolas, monospace; font-size: 16px; }",
		"Last geometry": ""
	},
	"Converter": {
		"Markdown format": {
			"bullet": "-",
			"listItemIndent": "one",
			"ruleRepetition": 10,
			"tightDefinitions": true,
			"fences": true
		},
		"Definition lists": true,
		"Inline media": true,
		"Table style": "extended",
		"Table newline": "Â¨",
		"Hard break": "spaces",
		"Inlines": [{
				"markdownSymbol": "_",
				"mdastNode": "underline",
				"htmlNode": "u"
			},{
				"markdownSymbol": "^",
				"mdastNode": "superscript",
				"htmlNode": "sup"
			},{
				"markdownSymbol": "~",
				"mdastNode": "subscript",
				"htmlNode": "sub"
			},{
				"markdownSymbol": "~~",
				"mdastNode": "strikethrough",
				"htmlNode": "del"
		}]
	},
	"CodeMirror": {
		"keymap": [{
				"key": "Mod-f",
				"run": "openSearchPanel",
				"scope": "editor search-panel",
				"preventDefault": true
			},{
				"key": "Mod-n",
				"run": "findNext",
				"shift": "findPrevious",
				"scope": "editor search-panel",
				"preventDefault": true
			},{
				"key": "Mod-d",
				"run": "selectNextOccurrence",
				"preventDefault": true
			},{
				"key": "Mod-Shift-l",
				"run": "selectSelectionMatches",
				"preventDefault": true
			},{
				"key": "Mod-g",
				"run": "gotoLine",
				"preventDefault": true
			},{
				"key": "Escape",
				"run": "closeSearchPanel",
				"scope": "editor search-panel",
				"preventDefault": true
			},{
				"key": "Mod-Alt-Shift-c",
				"run": "clozeCurrent",
				"preventDefault": true
			},{
				"key": "Mod-Shift-c",
				"run": "clozeNext",
				"preventDefault": true
			}
		]
	}

}